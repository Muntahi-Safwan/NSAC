# Database Configuration for Data Processing Pipeline
# Copy this to .env and update if needed

# Database connection (for Docker container)
DATABASE_URL="postgresql://postgres:postgres@postgres:5432/airquality?schema=public"

# For running outside Docker (localhost)
# DATABASE_URL="postgresql://postgres:postgres@localhost:5432/airquality?schema=public"

# PostgreSQL credentials (match database/.env)
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=airquality
POSTGRES_PORT=5432


